{
  "output": {
    "filePath": "repomix-output.xml",
    "style": "xml"
  },
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "*.wav",
      "*.mp3",
      "*.mp4",
      "*.png",
      "*.jpg",
      "*.jpeg",
      "*.gif",
      "*.ico",
      "*.svg",
      "*.pyc",
      "*.pyo",
      "*.egg-info",
      "*.egg",
      "*.whl",
      "__pycache__/",
      ".git/",
      ".venv/",
      "venv/",
      "env/",
      "node_modules/",
      ".pytest_cache/",
      ".mypy_cache/",
      "*.log",
      "logs/",
      "data/",
      "test_*.py",
      "*_test.py",
      "tests/",
      "test/",
      "*.test.js",
      "*.spec.js",
      "assets/",
      "scripts/",
      "docs/",
      "*.md",
      "*.txt",
      "*.json",
      "*.xml",
      "*.yaml",
      "*.yml",
      "*.lock",
      "*.css",
      ".env*",
      "Dockerfile*",
      "docker-compose*",
      "Makefile",
      "requirements*.txt",
      "setup.py",
      "setup.cfg",
      "pyproject.toml",
      "*.html",
      "static/",
      "repomix-output.xml",
      "repomix.config.json"
    ]
  },
  "include": [
    "server/**/*.py",
    "client/**/*.py"
  ]
}


